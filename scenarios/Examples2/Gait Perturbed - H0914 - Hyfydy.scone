CmaOptimizer {
	signature_prefix = DATE_TIME
	min_progress = 1e-4
	
	SimulationObjective {
		max_duration = 30
		auto_std_factor = 0.02
		
		# Model used in simulation
		ModelHyfydy {
			model_file = data/H0914.hfd
			
			# Optimize initial state parameters
			state_init_file = data/InitStateGait10.zml
			initial_state_offset = 0~0.01<-0.5,0.5>
			initial_state_offset_exclude = "*_tx;*_ty;*_u"
			fixed_control_step_size = 0.005
			
			# Required for Perturbations
			enable_external_forces = 1
		}
		
		# Controller for perturbed gait
		CompositeController {
			# Controller for gait, based on [Geyer & Herr 2010]
			<< data/ControllerGH2010v9.scone >>
			
			# Perturbate each body in rotating order
			ScriptController {
				name = Perturbation
				script_file = data/ScriptControllerPerturbation.lua
				start_time = 5
				force = 150
				interval = 0.5
			}
		}
		
		# Measure for gait
		<< data/MeasureGait10Grf15.scone >>
	}
}
